{
  "name": "node-logger-plus",
  "version": "0.2.6",
  "description": "node console 有趣的打印 log print 打印颜色 style log 美化 带风格的日志",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "nodemon __test__/dist/index.js",
    "start": "node __test__/dist/index.js",
    "test": "rspack build --config rspack.test.cjs",
    "cjs": "rspack build --config rspack.config.cjs",
    "esm": "rspack build --config rspack.config.mjs",
    "dts": "npx tsc -w"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "node",
    "console",
    "有趣的打印",
    "log",
    "print",
    "打印颜色",
    "style",
    "log",
    "美化",
    "带风格的日志"
  ],
  "license": "ISC",
  "files": [
    "README.md",
    "package.json",
    "dist"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/com-hooks/node-console"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "bus-plus": "^0.2.3"
  }
}